{
    "manifest_version": 3,
    "name": "LinkedIn Profile Saver",
    "version": "1.0",
    "description": "Open LinkedIn Profile in New Tab",
    "permissions": [
      "storage",
      "activeTab",
     "webNavigation",
    "downloads"
    ],
    "action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  